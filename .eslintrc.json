{
	"extends": [
		"airbnb",
		"prettier",
		"plugin:node/recommended"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off",
		"no-process-exit": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off",
		"react/no-unknown-property": [
			0,
			{
				"ignore": [
					"class"
				]
			}
		]
	}
}
